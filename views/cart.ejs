<script>

    function changequantity(cartId,prodId, count) {
        const quantity = parseInt(document.getElementById(prodId).value)
        console.log('add to cart working')
        $.ajax({
            url: '/changequantity',
            data : {
                cartId : cartId,
                prodId : prodId,
                count : count,
                quantity : quantity
            },
            method: 'post',
            
            success: (response) => {

                
                
                
                if(response.removeProduct) {
                    // document.getElementById('dec').innerHTML
                    document.getElementById('total').innerHTML = response.totalAmount
                    // location.reload()
                }else {
                    // window. stop();
                    // document.getElementById(prodId).innerHTML =  quantity + count
                    document.getElementById(prodId).value = quantity + count
                    document.getElementById('total').innerHTML = response.totalAmount
                    document.getElementById('subtotal').innerHTML = response.totalAmount
                    


                    
                }
            }
        })
    }
    
    </script>

